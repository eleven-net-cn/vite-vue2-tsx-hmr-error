{
  "name": "dependency-cruiser-demo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --port 4000",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "depcruise src --config .dependency-cruiser.cjs"
  },
  "gitHooks": {
    "pre-commit": "lint-staged --allow-empty"
  },
  "lint-staged": {
    "src/**/*.{js?(x),ts?(x)}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "dayjs": "^1.11.4",
    "lodash": "^4.17.21",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@vitejs/plugin-vue": "^3.0.0",
    "dependency-cruiser": "^11.12.0",
    "lint-staged": "^13.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vue-tsc": "^0.38.4",
    "yorkie": "^2.0.0"
  }
}
