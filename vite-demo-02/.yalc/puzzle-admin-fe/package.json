{
  "name": "puzzle-admin-fe",
  "version": "1.16.9",
  "description": "Puzzle 可视化搭建",
  "scripts": {
    "start": "webpack-dev-server --mode=development --config ./build/webpack.config.js --env development --host 0.0.0.0",
    "build": "webpack --progress --mode=production --config ./build/webpack.config.js --env production",
    "build:devtools": "rm -rf dist_devtools/* && webpack --progress --mode=production --config ./build/webpack.devtools.config.js --env production",
    "pre": "webpack --progress --mode=production --config ./build/webpack.config.js --env prerelease",
    "test": "webpack --progress --mode=production --config ./build/webpack.config.js --env test",
    "clean": "node ./node_modules/rimraf/bin.js ./dist/*",
    "first": "git tag v1.0.0 && npm run postversion",
    "patch": "npm version patch -m \"UPGRADE TO %s [ONLY FOR PRODUCTION]\"",
    "ver": "npm version $v -m \"UPGRADE TO %s [ONLY FOR PRODUCTION]\"",
    "postversion": "git push --follow-tags",
    "analyz": "analyz=true npm run build",
    "server": "node-dev server.js",
    "lint": "npm run elint",
    "lintFix": "npm run elintFix",
    "elint": "npx eslint \"./src/**/*.vue\" \"./src/**/*.js\"",
    "elintFix": "npx eslint \"./src/**/*.vue\" \"./src/**/*.js\" \"./src/**/*.ts\" --fix"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "mihoyo",
  "license": "ISC",
  "dependencies": {
    "@me/audio": "^2.1.4",
    "@me/utils": "^1.0.8",
    "@puzzle-admin/upload": "^1.1.10",
    "@puzzle/core": "^1.2.1",
    "@puzzle/utils": "^1.0.1",
    "@puzzle/wrapper": "^2.2.35",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "component-emitter": "^1.3.0",
    "diff": "^5.0.0",
    "diff2html": "^3.4.13",
    "element-ui": "^2.15.6",
    "file-saver": "^2.0.5",
    "hotkeys-js": "^3.8.3",
    "jquery": "^3.5.1",
    "jszip": "^3.9.1",
    "jszip-utils": "^0.1.0",
    "lodash": "^4.17.20",
    "mihoyo-admin-cookie": "^1.0.7",
    "mihoyo-admin-login": "^1.2.21",
    "mihoyo-admin-model": "^1.2.35",
    "mihoyo-auth": "^3.0.2",
    "mihoyo-browser": "^1.2.3",
    "mihoyo-clipboard": "^1.0.3",
