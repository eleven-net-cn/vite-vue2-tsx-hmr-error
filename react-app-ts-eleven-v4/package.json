{
  "name": "react-app-ts-eleven-v4",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn build-prod",
    "build-prod": "env-cmd -e production craco build",
    "build-test": "env-cmd -e test craco build",
    "build-uat": "env-cmd -e uat craco build",
    "build:analyze": "cross-env BUILD_ANALYZER=true yarn build-prod",
    "changelog": "standard-version -n",
    "commit": "git-cz",
    "start": "env-cmd -e development craco start",
    "test": "craco test"
  },
  "lint-staged": {
    "**/*.{js,jsx,json,css,scss,less,html,md}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "prettier --parser=typescript --write"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "./node_modules/@umijs/fabric/dist/eslint"
    ],
    "rules": {
      "array-callback-return": 0,
      "consistent-return": 0,
      "no-nested-ternary": 0,
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "no-underscore-dangle": 0,
      "no-unused-expressions": 0,
      "prefer-destructuring": 0,
      "prefer-object-spread": 0,
      "@typescript-eslint/consistent-type-definitions": 0,
      "@typescript-eslint/consistent-type-imports": 0,
      "@typescript-eslint/no-invalid-this": 0,
      "@typescript-eslint/no-shadow": 0,
      "@typescript-eslint/no-unused-vars": 1,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/alt-text": 0,
      "jsx-a11y/anchor-has-content": 0,
      "jsx-a11y/aria-role": 0,
      "jsx-a11y/control-has-associated-label": 0,
      "react/button-has-type": 0,
      "react/react-in-jsx-scope": 0,
      "react/sort-comp": 0
    }
  },
  "eslintIgnore": [
    "src/setupProxy.js"
  ],
  "dependencies": {
    "@eleven.fe/reset.css": "^1.0.2",
    "antd": "^4.19.2",
    "axios": "^0.26.1",
    "core-js": "^3.21.1",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^4.0.3",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@craco/craco": "^6.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^18.0.0",
    "@umijs/fabric": "^2.5.13",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "circular-dependency-plugin": "^5.2.0",
    "commitizen": "^4.2.1",
    "craco-less": "^1.17.0",
    "craco-plugin-scoped-css": "^1.1.1",
    "craco-scoped-less": "^1.0.3",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "fs-extra": "^10.0.1",
    "generic-names": "^3.0.0",
    "http-proxy-middleware": "^1.0.5",
    "lint-staged": "^11.0.0",
    "postcss-less": "^3.1.4",
    "postcss-scss": "^3.0.0",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "typescript": "^4.4.2",
    "vconsole-webpack-plugin": "^1.5.2",
    "web-vitals": "^2.1.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpackbar": "^5.0.0-3",
    "yorkie": "^2.0.0"
  },
  "antdTheme": {},
  "gitHooks": {
    "pre-commit": "lint-staged --allow-empty",
    "commit-msg": "commitlint -E GIT_PARAMS"
  }
}
